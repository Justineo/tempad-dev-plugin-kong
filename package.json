{
  "name": "tempad-dev-plugin-kong",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.14.0",
  "description": "Kong UI plugins for TemPad Dev.",
  "author": "Justineo <justice360@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Justineo/tempad-dev-plugin-kong#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Justineo/tempad-dev-plugin-kong.git"
  },
  "bugs": "https://github.com/Justineo/tempad-dev-plugin-kong/issues",
  "keywords": [],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@antfu/ni": "^0.23.2",
    "@antfu/utils": "^0.7.10",
    "@figma/plugin-typings": "^1.106.0",
    "@kong/design-tokens": "^1.17.2",
    "@tempad-dev/plugins": "^0.3.2",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "lint-staged": "^15.2.11",
    "pnpm": "^9.15.1",
    "prettier": "^3.4.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vite": "^5.4.11"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm build && git add dist"
  },
  "lint-staged": {
    "*": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  }
}
